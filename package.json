{
  "name": "share-scribe",
  "version": "1.0.0",
  "description": "A simple notes sharing application",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src/**/*.ts --fix",
    "gen-env": "gen-env-types .env -o src/env.d.ts -e .",
    "build": "tsc",
    "start": "node dist/index.js",
    "start-alt": "ts-node src/index.ts",
    "dev": "nodemon dist/index.js",
    "dev-alt": "nodemon --exec ts-node src/index.ts",
    "dev-run": "concurrently \"npm run watch\" \"npm run dev\"",
    "watch": "tsc -w",
    "typeorm": "ts-node ./node_modules/.bin/typeorm -d ./src/config/database/ormconfig.ts",
    "migration:generate": "npm run typeorm migration:generate -o ./src/config/database/Migrations/migration",
    "migration:revert": "npm run typeorm migration:revert",
    "migration:run": "npm run typeorm migration:run",
    "migration:show": "npm run typeorm migration:show",
    "migration:drop": "npm run typeorm schema:drop",
    "swagger": "tsoa spec"
  },
  "keywords": [],
  "author": "Majji Kishore",
  "license": "ISC",
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv-safe": "^8.2.0",
    "express": "^4.18.2",
    "express-jwt": "^8.4.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "swagger-ui-express": "^5.0.0",
    "tsoa": "^6.0.0",
    "typeorm": "^0.3.19"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/dotenv-safe": "^8.1.5",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.6",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.1",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-promise": "^6.1.1",
    "gen-env-types": "^1.3.4",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "eslintConfig": {
    "extends": "eslint:recommended"
  }
}